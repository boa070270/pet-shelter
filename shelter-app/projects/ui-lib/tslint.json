{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "kebab-case"
    ],
    "no-inferrable-types": [
      true,
      "ignore-properties"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "no-input-rename": [
      false
    ],
    "no-output-rename": [
      false
    ]
  }
}
